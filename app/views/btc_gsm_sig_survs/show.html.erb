<%- model_class = BtcGsmSigSurv -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @btc_gsm_sig_surv.user_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:sig_str) %>:</strong></dt>
  <dd><%= @btc_gsm_sig_surv.sig_str %></dd>
  <dt><strong><%= model_class.human_attribute_name(:latitude) %>:</strong></dt>
  <dd><%= @btc_gsm_sig_surv.latitude %></dd>
  <dt><strong><%= model_class.human_attribute_name(:longitude) %>:</strong></dt>
  <dd><%= @btc_gsm_sig_surv.longitude %></dd>
  <dt><strong><%= model_class.human_attribute_name(:pic_timestamp) %>:</strong></dt>
  <dd><%= @btc_gsm_sig_surv.pic_timestamp %></dd>
  <dt><strong><%= model_class.human_attribute_name(:audit_create_date) %>:</strong></dt>
  <dd><%= @btc_gsm_sig_surv.audit_create_date %></dd>
  <dt><strong><%= model_class.human_attribute_name(:audit_change_date) %>:</strong></dt>
  <dd><%= @btc_gsm_sig_surv.audit_change_date %></dd>

  <!--
  <dt><strong><%= model_class.human_attribute_name(:userinfo) %>:</strong></dt>
  <dd><%= @btc_gsm_sig_surv.userinfo %></dd>
  -->
  <% JSON.parse(@btc_gsm_sig_surv.userinfo).each do |item| %>
    <dt><strong><%= model_class.human_attribute_name(item[0]) %>:</strong></dt>
    <dd><%= item[1] %></dd>
  <% end %>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              btc_gsm_sig_survs_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_btc_gsm_sig_surv_path(@btc_gsm_sig_surv), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              btc_gsm_sig_surv_path(@btc_gsm_sig_surv),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
