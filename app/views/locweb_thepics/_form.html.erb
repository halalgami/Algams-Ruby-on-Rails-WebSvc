<%= semantic_form_for @locweb_thepic do |f| %>
  <%= f.inputs do %>
    <%= f.input :posted_by %>
    <%= f.input :pic_name %>
    <%= f.input :pic_location_name %>
    <%= f.input :pic_coords %>
    <%= f.input :pic_timestamp %>
    <%= f.input :pic_image_path %>
    <%= f.input :pic_weather_currently %>
    <%= f.input :pic_status %>
    <%= f.input :pic_link %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
